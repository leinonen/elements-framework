{
  "name": "elements-micro-framework",
  "version": "1.0.0",
  "description": "Elements micro DOM framework",
  "main": "lib/app.js",
  "scripts": {
    "clean": "rimraf lib",
    "build": "webpack --mode=build",
    "dev": "webpack --progress --colors --watch --mode=dev",
    "devNoWatch": "webpack --progress --colors --mode=dev",
    "buildAll": "npm run build && npm run devNoWatch",
    "test": "cross-env NODE_ENV=test karma start karma.conf.js --single-run",
    "start": "node server/server.js"
  },
  "devDependencies": {
    "babel-cli": "6.14.0",
    "babel-core": "6.14.0",
    "babel-eslint": "6.1.2",
    "babel-loader": "6.2.5",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-preset-es2015": "6.14.0",
    "chai": "3.4.1",
    "cross-env": "^3.1.2",
    "css-loader": "^0.25.0",
    "eslint": "1.7.2",
    "eslint-loader": "1.1.0",
    "express": "^4.14.0",
    "isparta-loader": "^2.0.0",
    "karma": "^1.3.0",
    "karma-coverage": "^1.1.1",
    "karma-mocha": "^1.2.0",
    "karma-mocha-reporter": "^2.2.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.8.0",
    "minimist": "^1.2.0",
    "mocha": "2.3.4",
    "phantomjs-prebuilt": "^2.1.13",
    "rimraf": "^2.5.4",
    "webpack": "^1.12.9",
    "webpack-dev-middleware": "^1.8.4",
    "webpack-hot-middleware": "^2.13.0",
    "yargs": "3.32.0"
  },
  "keywords": [
    "webpack",
    "es6",
    "dom",
    "micro",
    "framework",
    "universal",
    "umd",
    "commonjs"
  ],
  "author": "Peter Leinonen",
  "license": "MIT",
  "dependencies": {
    "milligram": "^1.1.0"
  }
}
